<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Subject" table="tb_subject"><!-- 每个class对应一个持久化对象 -->
		<id name="subjectID"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="identity"></generator>
		</id>
		<property name="subjectTitle"></property>	<!-- 映射subjectTitle属性 -->
		<property name="subjectOptionA"></property>	<!-- 映射subjectOptionA属性 -->
		<property name="subjectOptionB"></property>	<!-- 映射subjectOptionB属性 -->
		<property name="subjectOptionC"></property>	<!-- 映射subjectOptionC属性 -->
		<property name="subjectOptionD"></property>	<!-- 映射subjectOptionD属性 -->
		<property name="subjectAnswer"></property>	<!-- 映射subjectAnswer属性 -->
		<property name="subjectParse"></property>	<!-- 映射subjectParse属性 -->
	</class>	
</hibernate-mapping>